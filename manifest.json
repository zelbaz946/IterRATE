{
    "name": "iterRATE",
    "description": "A chrome extension that calculates total time spent on coding websites and gives you a digital age",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons":{
        "16": "./icons/icon16.png",
        "24": "./icons/icon24.png",
        "32": "./icons/icon32.png",
        "128": "./icons/icon128.png"
    },
    "background": {
        "scripts": ["./background.js", "./foreground.js"]
    },
    "page_action": {
        "default_popup": "/options.html"
    },
    "permissions": [
        "tabs",
        "https://csx.codesmith.io/*",
        "https://leetcode.com/*",
        "https://www.codewars.com/*",
        "https://developer.mozilla.org/*",
        "storage"
    ],
    "content_security_policy" : "script-src 'self' https://cdn.jsdelivr.net; object-src 'self"
}
